{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "target": "ES6",
    "moduleResolution": "Node10",
    "module": "CommonJS",
    "declaration": true,
    "isolatedModules": true,
    "noEmit": true,
    "outDir": "dist",
    "jsx": "react-jsx"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
